{
  "name": "Ad Highlighter",
  "description": "An extension that highlights advertisements in text, image, and video form.",
  "version": "0.0.2",
  "background": {
        "service_worker": "background.js"
    },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {},
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 3
}
