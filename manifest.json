{
  "name": "Ad Highlighter",
  "description": "An extension that highlights banner advertisements (and some videos that may be ads).",
  "version": "0.0.1",
  "background": {
        "service_worker": "background.js"
    },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {},
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 3
}
