{
  "name": "Ad Highlighter",
  "description": "An extension that highlights banner advertisements (and some video ads).",
  "version": "0.0.1",
  "content_scripts": [
   {
    "matches": [
      "<all_urls>",
      "https://chromewebstore.google.com/detail/adblock-%E2%80%94-block-ads-acros/gighmmpiobklfepjocnamgkkbiglidom?pli=1"
    ],

     "js": ["scripts/content.js"]
   }
 ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 3
}
